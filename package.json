{
  "name": "vue-gapi",
  "version": "0.1.1",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "demo": "vue-cli-service demo",
    "docs": "npm run docs:serve",
    "docs:build": "vue-cli-service docs --mode build",
    "docs:serve": "vue-cli-service docs --mode serve",
    "prepublish": "vue-cli-service lint && vue-cli-service docs --mode build && vue-cli-service build",
    "start": "vue-cli-service serve"
  },
  "main": "dist/vue-gapi.common.js",
  "module": "dist/vue-gapi.esm.js",
  "unpkg": "dist/vue-gapi.umd.min.js",
  "files": [
    "dist/vue-gapi.common.js",
    "dist/vue-gapi.umd.min.js",
    "dist/vue-gapi.umd.js",
    "dist/vue-gapi.esm.js",
    "src"
  ],
  "dependencies": {
    "core-js": "^3.4.4",
    "secure-ls": "^1.2.6"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^7.12.1",
    "eslint-plugin-vue": "^7.1.0",
    "vue": "^2.6.10",
    "vue-cli-plugin-p11n": "^0.4.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off",
      "no-unused-vars": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "jsdelivr": "dist/vue-gapi.umd.min.js",
  "sideeffects": false
}
